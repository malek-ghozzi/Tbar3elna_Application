<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tbar3ilna">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/tbar3ilna_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/tbar3ilna_icon_foreground"
        android:supportsRtl="true"
        android:theme="@style/Theme.Tbar3ilna">
        <activity android:name=".DonationReq" />
        <activity android:name=".LoginAdmin" />
        <activity android:name=".Moreinfo" />
        <activity android:name=".Feed" />
        <activity android:name=".Welcome" />
        <activity android:name=".Signin" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver android:name=".NotificationReciver"/>
    </application>

</manifest>